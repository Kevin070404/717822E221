// Package for entity classes
package entities;

public class Faculty {
    // Fields for Faculty entity
    private int id;
    private String name;
    private String email;

    // Constructor
    public Faculty(int id, String name, String email) {
        this.id = id;
        this.name = name;
        this.email = email;
    }

    // Getter methods
    public int getId() {
        return id;
    }

    public String getName() {
        return name;
    }

    public String getEmail() {
        return email;
    }
}
